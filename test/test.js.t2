;; add canvas
var canvas = document.createElement('canvas');
canvas.id = 'canvas';

document.body.appendChild(canvas);

;; resize canvas
canvas.width = 640;
canvas.height = 480;

;; draw in canvas
; get context
; draw rects

;; get context
ctx = canvas.getContext('2d');
console.log(ctx);

;; draw rects
ctx.fillStyle = "rgb(200 0 0)";
ctx.fillRect(0, 0, 640, 480);
